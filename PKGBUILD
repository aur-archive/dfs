pkgname=dfs
pkgver=1.0
pkgrel=2
pkgdesc='Simple bash script that displays partition usage in a graphic format.'
arch=('any')
license=('GPL')
depends=('gawk' 'coreutils') 
source=($pkgname)
url='http://aur.archlinux.org/packages.php?ID=51752'

build() {
	install -D -m755 $pkgname $pkgdir/usr/bin/$pkgname
}
